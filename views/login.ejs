<%- include ("./partials/header") %>
<%- include("./partials/sidebar") %>

<div class="wrap">

<div class="loginWrapper fadeInDown">
  <div class="loginContent">
    <!-- Tabs Titles -->

    <!-- Icon -->
    <div class="">
		<i class="mr-3 mt-3 rounded-circle"></i>
    </div>
    <!-- Login Form -->
    <form action="/login" method="POST">
      <input class="loginInputText" type="text" id="login" name="username" placeholder="username">
      <input class="loginInputText" type="password" id="password" name="password" placeholder="password">
      <input class="loginButton" type="submit" value="Log In">
    </form>

    <!-- Remind Passowrd -->
    <div class="loginFooter">
      <a class="underlineHover" href="#">Forgot Password?</a>
    </div>

  </div>
</div>

</div>

<%- include ("./partials/footer") %>